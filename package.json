{
  "name": "explore-with-iss",
  "version": "1.0.0",
  "description": "A live data visualization to explore and discover countries by following ISS in its route",
  "engines": {
    "node": "7.0.0",
    "npm": "3.10.8"
  },
  "homepage": "http://vogelino.github.io/explore-with-iss/",
  "repository": {
    "type": "git",
    "url": "git://github.com/vogelino/explore-with-iss.git"
  },
  "scripts": {
    "clean": "rimraf ./public && mkdir ./public && mkdir ./public/images",
    "watch:client": "webpack -w",
    "watch:server": "nodemon --exec babel-node ./src/server.js",
    "lint:js": "eslint ./src/client/js/**.js",
    "build:client:images:flags": "cpx src/client/images/flags/**/* public/images",
    "build:client:images:favicons": "cpx src/client/images/favicons/**/* public",
    "build:client:images": "npm run build:client:images:flags & npm run build:client:images:favicons",
    "build:client:js": "webpack --progress",
    "build:client": "npm run build:client:js & npm run build:client:images",
    "build": "export NODE_ENV=production && npm run clean && npm run build:client",
    "watch": "export NODE_ENV=development && npm run lint:js && npm run build && npm run watch:server & npm run watch:client",
    "serve:server": "babel-node ./src/server.js",
    "serve": "npm run serve:server",
    "createDocs:js": "esdoc -c esdoc.json",
    "start": "export NODE_ENV=production && npm run serve"
  },
  "dependencies": {
    "autoprefixer": "6.3.3",
    "babel-cli": "6.5.1",
    "babel-core": "6.3.17",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.0",
    "babel-plugin-transform-runtime": "6.3.13",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-runtime": "6.3.19",
    "config": "1.19.0",
    "cpx": "1.5.0",
    "css-loader": "0.23.1",
    "d3": "3.5.14",
    "deferred-js": "1.0.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "leaflet": "0.7.7",
    "marked": "0.3.5",
    "nib": "1.1.0",
    "node-vibrant": "2.0.2",
    "nodemon": "1.9.0",
    "postcss": "5.0.16",
    "postcss-cli": "2.5.1",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "react-helmet": "2.3.1",
    "react-leaflet": "0.10.0",
    "react-redux": "4.0.1",
    "react-router": "2.0.0",
    "redux": "3.0.5",
    "request": "2.69.0",
    "rimraf": "^2.6.1",
    "socket.io": "1.4.5",
    "style-loader": "0.13.0",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "visibility": "0.0.0",
    "webpack": "1.14.0",
    "webpack-validator": "^3.0.0"
  },
  "license": "WTFPL"
}
