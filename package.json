{
  "name": "explore-with-iss",
  "version": "1.0.0",
  "description": "A live data visualization to explore and discover countries by following ISS in its route",
  "engines": {
    "node": "0.12.2",
    "npm": "2.7.4"
  },
  "homepage": "http://vogelino.github.io/explore-with-iss/",
  "repository": {
    "type": "git",
    "url": "git://github.com/vogelino/explore-with-iss.git"
  },
  "scripts": {
    "clean": "rm -rf ./public && mkdir ./public",
    "watch:client": "./node_modules/.bin/webpack -w",
    "watch:server": "./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node app/server.js",
    "lint:js": "./node_modules/.bin/eslint ./src/client/js/**.js",
    "build:client:js": "npm run lint:js && ./node_modules/.bin/webpack",
    "build:client:styles:stylus": "./node_modules/.bin/stylus ./src/client/styles/index.styl -o ./public/bundle.css",
    "build:client:styles:autoprefixer": "./node_modules/.bin/postcss --use autoprefixer ./public/bundle.css -o ./public/bundle.css",
    "build:client:styles": "npm run build:styles:stylus && npm run build:styles:autoprefixer",
    "build:client": "npm run build:client:js & npm run build:client:styles",
    "build": "npm run clean && npm run build:client",
    "watch": "npm run build && npm run watch:server & npm run watch:client",
    "createDocs:js": "esdoc -c esdoc.json"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta8",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "clean-webpack-plugin": "^0.1.6",
    "copy-webpack-plugin": "^1.1.1",
    "eslint": "^1.10.3",
    "file-loader": "^0.8.5",
    "nib": "^1.1.0",
    "nodemon": "^1.9.0",
    "raw-loader": "^0.5.1",
    "stylus": "^0.53.0",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.2",
    "cors": "^2.7.1",
    "d3": "^3.5.14",
    "deferred-js": "^1.0.2",
    "express": "^4.13.4",
    "leaflet": "^0.7.7",
    "node-vibrant": "^2.0.2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-leaflet": "^0.10.0",
    "react-redux": "^4.0.1",
    "redux": "^3.0.5",
    "request": "^2.69.0"
  },
  "license": "WTFPL"
}
